  "version": "0.2.0",
  "configurations": [
      {
          "name": "Tezda Dev",
          "request": "launch",
          "type": "dart",
          "args": [
              "--dart-define",
              "app.env=dev"
          ]
      },
      {
          "name": "Tezda Prod",
          "request": "launch",
          "type": "dart",
          "args": [
              "--dart-define",
              "app.env=prod"
          ]
      },
      {
          "name": "Tezda (profile mode)",
          "request": "launch",
          "type": "dart",
          "flutterMode": "profile",
          "args": [
              "--dart-define",
              "app.env=prod"
          ]
      },
      {
          "name": "Tezda (release mode)",
          "request": "launch",
          "type": "dart",
          "flutterMode": "release",
          "args": [
              "--dart-define",
              "app.env=prod"
          ]
      }
  ]
}